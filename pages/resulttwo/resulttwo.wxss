page{
  height:100%;
  width:100%;
  background-color: #379e72;
}
.main_bg{
  width:100%;
  height:434rpx;
}
.main_bg image{
  width:100%;
  height:100%;
}
.container{
   width:700rpx;
   margin-left:25rpx;
}
.play_one_title{
  width:700rpx;
  height:107rpx;
  margin:0 auto;
  margin-top:-56rpx;
}
.table_title{
  height:90rpx;
  line-height: 90rpx;
  background-color: #d7ece3;
  margin-top:-12rpx;
  font-size: 30rpx;
  color:#379e72;
}
.table_title view{
  float:left;
  height:90rpx;
  line-height: 90rpx;
  text-align: center;
}
.rank{
  width:160rpx;
}
.name{
  width:120rpx;
}
.tel{
  width:220rpx;
}
.num{
  width:200rpx;
}
.table_content{
  background: #f1f8f5;
  overflow: hidden;
  border-radius: 12rpx;
}
.table_content view{
  float:left;
  height:90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size:26rpx;
}
.table_content image{
  margin-top:10rpx;
  width:52rpx;
  height:70rpx;
}
.table_content .num{
  color:#ce1d1d;
  font-size: 38rpx;
}
.table_content .rank{
  font-size: 38rpx;
  font-style:italic
}
.table_content .tr{
  clear: both;
}
.detail{
  clear: both;
  height:90rpx;
  width:100%;
  text-indent: 10px;
  background: #d7ece3;
  font-size:22rpx;
  text-align: left!important;
  color:#999;
}
.container_second{
  margin-top:30rpx;
}
.play_two_title{
  width:700rpx;
  height:107rpx;
  margin:0 auto;
}
.forcast{
  margin-top:10rpx;
  width:700rpx;
  margin-left:25rpx;
  font-size: 24rpx;
  color:#f1f8f5;
}
.bottom_img{
  width:750rpx;
  height:169rpx;
  vertical-align: bottom
}

.winn_btn{
  width: 334rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 34rpx;
  border: 2rpx solid #fff;
  background: #379E72;
  color: #fff;
  box-shadow: 0 0 5rpx #fff;
  letter-spacing: 0.08em;
  margin: 70rpx auto;
}

.winn_btn::after{
  border: none;
}
.leftArrow{
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx
}